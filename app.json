{
  "expo": {
    "scheme": "internapp3",
    "name": "internApp3",
    "slug": "internApp3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ayushsingh.internApp3"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "326d511b-2ba1-42d8-ab47-220c23655519"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/326d511b-2ba1-42d8-ab47-220c23655519"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Mulish/Mulish-Bold.ttf",
            "./assets/fonts/Mulish/Mulish-SemiBold.ttf",
            "./assets/fonts/Mulish/Mulish-ExtraBold.ttf",
            "./assets/fonts/Mulish/Mulish-ExtraLight.ttf",
            "./assets/fonts/Mulish/Mulish-Regular.ttf"
          ]
        }
      ]
    ]
  }
}
